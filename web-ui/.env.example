# Web UI 环境变量配置示例

# Bot API 连接配置
NEXT_PUBLIC_BOT_API_URL=http://localhost:8000
NEXT_PUBLIC_BOT_WS_URL=ws://localhost:8000/ws
NEXT_PUBLIC_BOT_API_KEY=your_bot_api_key_here

# 应用配置
NEXT_PUBLIC_APP_NAME="Potato Bot Dashboard"
NEXT_PUBLIC_APP_VERSION="2.2.0"

# API 基础配置
API_BASE_URL=http://localhost:8000/api/v1

# 开发模式设置
NODE_ENV=development

# 安全设置
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# 数据库连接（如果直接连接）
# DATABASE_URL=mysql://username:password@localhost:3306/potato_bot

# Redis 连接（可选）
# REDIS_URL=redis://localhost:6379

# 其他可选配置
# NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn_here
# NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id_here
